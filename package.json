{
  "name": "book-reading-list",
  "version": "1.0.0",
  "description": "Book reading list app",
  "keywords": [
    "PostgreSQL",
    "Sequelize",
    "Node",
    "React",
    "GraphQL",
    "MaterialUI"
  ],
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Mirko Mackovic",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "apollo-server-express": "^2.6.9",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "graphql": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^6.0.0"
  }
}
